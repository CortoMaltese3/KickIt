@{
    ViewData["Title"] = "Home Page";
}

@section Styles{

    <link rel="stylesheet" href="~/css/google.css" />
}

<input id="pac-input" class="controls" type="text" placeholder="Search Box">
<div id="map" style="height:400px"></div>
<button id="GetLocations" type="button" class="btn-dark">Get Locations</button>
<ul id="locations"></ul>
@section Scripts{
    <script src="~/js/googleapi.js"></script>
    <script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=false&libraries=geometry"></script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDrnm2ptgfajjtlCfM97wat7Jsx5KZ0UoA&libraries=places&callback=initAutocomplete"
            async defer></script>

}
